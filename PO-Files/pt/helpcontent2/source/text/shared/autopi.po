#: 01040000.xhp#bm_id3149031.help.text
msgid ""
"<bookmark_value>wizards;agendas</bookmark_value><bookmark_value>Agenda "
"Wizard</bookmark_value><bookmark_value>templates;agendas</bookmark_value>"
msgstr ""

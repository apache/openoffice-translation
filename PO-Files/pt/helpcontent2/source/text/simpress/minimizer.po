#: step2.xhp%23par_id77665505.help.text
msgid ""
"<ahelp hid=\"hid/sdext:Minimizer:Wizard:Step2\">Choose which slides to "
"delete.</ahelp>"
msgstr ""
"<ahelp hid=\"hid/sdext:Minimizer:Wizard:Step2\">Escolha os diapositivos a "
"eliminar.</ahelp>"

#: step2.xhp%23hd_id77665501.help.text
msgid "Delete unused master pages"
msgstr "Eliminar páginas mestras não utilizadas"

#: step2.xhp%23par_id77665503c.help.text
msgid ""
"<ahelp hid=\"sdext:Minimizer:Wizard:Step2:CBMasterPages\">Delete unused "
"master pages.</ahelp>"
msgstr ""
"<ahelp hid=\"sdext:Minimizer:Wizard:Step2:CBMasterPages\">Eliminar páginas "
"mestras não usadas.</ahelp>"

#: step2.xhp%23hd_id77665502.help.text
msgid "Delete hidden slides"
msgstr "Eliminar diapositivos ocultos"

#: step2.xhp%23par_id77665503d.help.text
msgid ""
"<ahelp hid=\"sdext:Minimizer:Wizard:Step2:CBHiddenSlides\">Delete hidden "
"slides.</ahelp>"
msgstr ""
"<ahelp hid=\"sdext:Minimizer:Wizard:Step2:CBHiddenSlides\">Eliminar "
"diapositivos ocultos.</ahelp>"

#: step2.xhp%23hd_id77665503.help.text
msgid "Delete slides that are not used for the custom slide show"
msgstr "Eliminar diapositivos não utilizados no diaporama personalizado"

#: step2.xhp%23par_id77665503e.help.text
msgid ""
"<ahelp hid=\"sdext:Minimizer:Wizard:Step2:CBCustomShow\">Delete slides that "
"are not used for the custom slide show.</ahelp>"
msgstr ""
"<ahelp hid=\"sdext:Minimizer:Wizard:Step2:CBCustomShow\">Eliminar "
"diapositivos não utilizados no diaporama personalizado.</ahelp>"

#: step2.xhp%23par_id77665503f.help.text
msgid ""
"<ahelp hid=\"sdext:Minimizer:Wizard:Step2:LBCustomShow\">This list box "
"displays all slides that are not used in the custom slide show.</ahelp>"
msgstr ""
"<ahelp hid=\"sdext:Minimizer:Wizard:Step2:LBCustomShow\">Esta lista mostra "
"todos os diapositivos não utilizados no diaporama personalizado.</ahelp>"

#: step2.xhp%23hd_id77665505.help.text
msgid "Clear notes"
msgstr "Limpar as notas"

#: step2.xhp%23par_id77665503g.help.text
msgid ""
"<ahelp hid=\"sdext:Minimizer:Wizard:Step2:CBNotes\">Clear the contents of "
"all notes.</ahelp>"
msgstr ""
"<ahelp hid=\"sdext:Minimizer:Wizard:Step2:CBNotes\">Limpar o conteúdo de "
"todas as notas.</ahelp>"


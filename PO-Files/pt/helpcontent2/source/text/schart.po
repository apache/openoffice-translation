msgstr ""
"<ahelp hid=\".\" visibility=\"hidden\">Formata os indicadores de perda de "
"cotações.</ahelp>"

#: main0000.xhp%23par_id0810200903545113.help.text
msgid ""
"<ahelp hid=\".\" visibility=\"hidden\">Formats the stock gain "
"indicators.</ahelp>"
msgstr ""
"<ahelp hid=\".\" visibility=\"hidden\">Formata os indicadores de ganho de "

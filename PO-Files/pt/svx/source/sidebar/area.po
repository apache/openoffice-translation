msgstr "~Transparência:"

#: AreaPropertyPanel.src%23RID_SIDEBAR_AREA_PANEL.STR_HELP_COLOR.string.text
msgid "Select the color to apply."
msgstr "Seleccione a cor a aplicar."

#: AreaPropertyPanel.src%23RID_SIDEBAR_AREA_PANEL.STR_HELP_TYPE.string.text
msgid "Select the fill type to apply."
msgstr "Seleccione o preenchimento a aplicar."

#: AreaPropertyPanel.src%23RID_SIDEBAR_AREA_PANEL.STR_HELP_ATTR.string.text
msgid "Select the effect to apply."
msgstr "Seleccione o efeito a aplicar."

#: AreaPropertyPanel.src%23RID_SIDEBAR_AREA_PANEL.LB_TRGR_TYPES.listbox.text

msgid "Year"
msgstr "Ano"

#: xforms.src%23RID_STR_DATATYPE_MONTHDAY.string.text
msgid "Month and day"
msgstr "Mês e dia"

#: xforms.src%23RID_STR_DATATYPE_MONTH.string.text
msgid "Month"
msgstr "Mês"

#: xforms.src%23RID_STR_DATATYPE_DAY.string.text

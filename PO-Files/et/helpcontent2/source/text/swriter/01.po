msgstr ""
"<ahelp hid=\"SW_MENUBUTTON_TP_TOX_SELECT_MB_AUTOMARK\">Vali, loo või muuda "
"registrifaili.</ahelp>"

#: 04120212.xhp%23hd_id3152950.32.help.text
msgctxt "04120212.xhp#hd_id3152950.32.help.text"
msgid "Sort"
msgstr "Sordi"

#: 04120212.xhp%23par_id3149812.33.help.text

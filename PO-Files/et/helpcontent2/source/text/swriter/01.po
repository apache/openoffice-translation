msgid ""
"<ahelp hid=\"SW:EDIT:DLG_CONV_TEXT_TABLE:ED_OTHER\">Type the character that "
"you want to use as a column marker.</ahelp>"
msgstr ""
"<ahelp hid=\"SW:EDIT:DLG_CONV_TEXT_TABLE:ED_OTHER\">Sisesta märk, mida "
"soovid kasutada veergude eraldajana.</ahelp>"

#: 06090000.xhp%23hd_id3147508.18.help.text
msgid "Equal width for all columns"
msgstr "Kõigile veergudele võrdne laius"


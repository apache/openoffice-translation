msgstr ""
"<ahelp hid=\"SW:CHECKBOX:DLG_CONV_TEXT_TABLE:CB_DONT_SPLIT\">Ei tükelda "
"tabelit mitmele leheküljele.</ahelp>"

#: 06090000.xhp%23hd_id3154472.26.help.text
msgctxt "06090000.xhp#hd_id3154472.26.help.text"
msgid "Border"
msgstr "Ääris"


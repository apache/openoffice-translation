msgstr "-"

#: AreaPropertyPanel.src#RID_SIDEBAR_AREA_PANEL.LB_TRGR_TYPES.listbox.quickhelptext
msgid "Select the type of transparency to apply."
msgstr "Seleccione el tipo de transparència per a aplicar."

#: AreaPropertyPanel.src#RID_SIDEBAR_AREA_PANEL.LB_TRGR_TYPES.1.stringlist.text
msgid "None"
msgstr "Cap"

#: AreaPropertyPanel.src#RID_SIDEBAR_AREA_PANEL.LB_TRGR_TYPES.2.stringlist.text
msgid "Solid"
msgstr "Sòlit"

#: AreaPropertyPanel.src#RID_SIDEBAR_AREA_PANEL.LB_TRGR_TYPES.3.stringlist.text
msgid "Linear"
msgstr "Llineal"

#: AreaPropertyPanel.src#RID_SIDEBAR_AREA_PANEL.LB_TRGR_TYPES.4.stringlist.text
msgid "Axial"
msgstr "Axial"

#: AreaPropertyPanel.src#RID_SIDEBAR_AREA_PANEL.LB_TRGR_TYPES.5.stringlist.text
msgid "Radial"
msgstr "Radial"

#: AreaPropertyPanel.src#RID_SIDEBAR_AREA_PANEL.LB_TRGR_TYPES.6.stringlist.text
msgid "Ellipsoid"
msgstr "Elipsoide"

#: AreaPropertyPanel.src#RID_SIDEBAR_AREA_PANEL.LB_TRGR_TYPES.7.stringlist.text
msgid "Quadratic"
msgstr "Quadràtic"

#: AreaPropertyPanel.src#RID_SIDEBAR_AREA_PANEL.LB_TRGR_TYPES.8.stringlist.text
msgid "Square"
msgstr "Quadrat"

#: AreaPropertyPanel.src#RID_SIDEBAR_AREA_PANEL.MTR_TRANSPARENT.metricfield.text

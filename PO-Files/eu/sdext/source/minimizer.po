msgstr "~Bikoiztu aurkezpena aldaketak aplikatu baino lehenago"

#: minimizer.src%23STR_DELETE_SLIDES.string.text
msgid "Delete %SLIDES slides."
msgstr "Ezabatu %SLIDES diapositibak"


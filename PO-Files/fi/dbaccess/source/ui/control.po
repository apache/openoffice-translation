msgid "Modify SQL statement(s)"
msgstr "Muuta SQL-lausetta tai -lauseita"

#: tabletree.src%23MENU_TABLETREE_POPUP.MID_SORT_ASCENDING.menuitem.text
msgid "Sort Ascending"
msgstr "Nouseva lajittelu"


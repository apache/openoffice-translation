msgstr "Ջնջիր սյունակը"

#: query.src#STR_QUERY_UNDO_TABFIELDMOVED.string.text
msgid "Move column"
msgstr "Տեղաշարժիր սյունակը"

#: query.src#STR_QUERY_UNDO_TABFIELDCREATE.string.text
msgid "Add Column"
msgstr "Ավելացրու սյունակը"

#: query.src#RID_STR_TABLE_DOESNT_EXIST.string.text
msgid "Invalid expression, table '$name$' does not exist."
msgstr "Անվավեր արտահայտություն, աղյուսակի '$name$' անունը գոյություն չունի։"

#: query.src#RID_STR_FIELD_DOESNT_EXIST.string.text
msgid "Invalid expression, field name '$name$' does not exist."
msgstr "Անվավեր արտահայտություն, '$name$' անունով դաշտ առկա չէ։"

#: query.src#RID_STR_TOMUCHTABLES.string.text
msgid ""
"The query covers #num# tables. The selected database type, however, can only "
"process a maximum of #maxnum# table(s) per statement."
msgstr ""
"Հարցումը պարունակում է  #num# աղյուսակ։ Ընտրված շտեմարանի տեսակը, "
"այնուամենայնիվ, կարող է մեկ հայտարարության համար մշակել առավելագույնը "
"#maxnum# աղյուսակ(ներ):"


msgid ""
"<switchinline select=\"sys\"><caseinline select=\"MAC\">Command "
"</caseinline><defaultinline>Ctrl</defaultinline></switchinline>+Minus key"
msgstr ""
"<switchinline select=\"sys\"><caseinline select=\"MAC\">Comando</caseinline> "
"<defaultinline>Ctrl</defaultinline></switchinline>+-"

#: 01020000.xhp%23par_id3147580.84.help.text
msgid "Send Backward."
msgstr "Porta più indietro."

#: 01020000.xhp%23hd_id3148742.85.help.text
msgid ""
"Shift+<switchinline select=\"sys\"><caseinline select=\"MAC\">Command "
"</caseinline><defaultinline>Ctrl</defaultinline></switchinline>+Minus key"
msgstr ""
"Maiusc+<switchinline select=\"sys\"><caseinline select=\"MAC\""
">Comando</caseinline> <defaultinline>Ctrl</defaultinline></switchinline>+-"

#: 01020000.xhp%23par_id3152775.86.help.text
msgid "Send to Back."
msgstr "Porta in fondo."


msgstr ""
"<ahelp hid=\".\" visibility=\"hidden\">Formuojamas pažymėtas duomenų "
"taškas.</ahelp>"

#: main0000.xhp%23par_id0810200903545096.help.text
msgid "<ahelp hid=\".\" visibility=\"hidden\">Formats the major grid.</ahelp>"
msgstr ""
"<ahelp hid=\".\" visibility=\"hidden\">Formuojamas pagrindinis "
